{
    "name": "ruslanstarikov/bowser-ai",
    "description": "Laravel utilities for OpenAI consumption",
    "type": "library",
    "require": {
        "openai-php/client": "^0.10.3",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel/framework": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ruslanstarikov\\BowserAi\\Providers\\BowserAiServiceProvider"
            ]
        }
    },
    "license": "UNLICENSE",
    "autoload": {
        "psr-4": {
            "Ruslanstarikov\\BowserAi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ruslan Starikov",
            "email": "ruslan.starikov@workyard.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
